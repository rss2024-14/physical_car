<launch>
    <node pkg="path_planning" exec="trajectory_planner" name="trajectory_planner" output="screen">
        <param from="$(find-pkg-share path_planning)/config/sim/sim_config.yaml"/>
        <param name="from_start_traj" value="$(find-pkg-share path_planning)/example_trajectories/from_start.traj"/>
        <param name="to_start_traj" value="$(find-pkg-share path_planning)/example_trajectories/to_start.traj"/>
    </node>

    <node pkg="path_planning" exec="trajectory_loader" name="trajectory_loader">
        <param name="trajectory1" value="$(find-pkg-share path_planning)/example_trajectories/from_start.traj"/>
        <param name="trajectory2" value="$(find-pkg-share path_planning)/example_trajectories/to_start.traj"/>
    </node>
</launch>
